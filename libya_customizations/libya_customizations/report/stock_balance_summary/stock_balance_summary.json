 "name": "Stock Balance Summary",
 "owner": "Administrator",
 "prepared_report": 0,
 "query": "WITH\nstock_ledger_entry AS (\n\tSELECT\n\t\titem_code,\n\t\tSUM(actual_qty) AS actual_qty\n\tFROM\n\t\t`tabStock Ledger Entry`\n\tWHERE\n\t\tis_cancelled = 0\n\tAND\n\t\tposting_date <= %(to_date)s\n\tGROUP BY\n\t\titem_code\n\tHAVING\n\t\tSUM(actual_qty) > 0\n)\nSELECT\n\tstock_ledger_entry.item_code,\n\titem.item_name,\n\titem.brand,\n\tstock_ledger_entry.actual_qty\nFROM\n\tstock_ledger_entry\nINNER JOIN\n\t`tabItem` item\nON\n\tstock_ledger_entry.item_code = item.name\nWHERE\n\titem.is_stock_item = 1\nAND\n\tstock_ledger_entry.actual_qty > 0\nORDER BY\n    item.brand",
 "ref_doctype": "Stock Ledger Entry",
