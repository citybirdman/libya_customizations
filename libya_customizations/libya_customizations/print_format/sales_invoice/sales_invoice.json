 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header,\r\n\t render_field -%}\r\n\r\n\r\n<div id=\"header-html\" >\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    <div style=\"\">\r\n        {% if doc.is_return and doc.is_paid %}\r\n            <p style=\" margin-left:30mm; display:\"> \u0641\u0627\u062a\u0648\u0631\u0629 \u0645\u0631\u062f\u0648\u062f\u0627\u062a \u0645\u0628\u064a\u0639\u0627\u062a (\u0646\u0642\u062f\u064a\u0629) \u0631\u0642\u0645 <span> <b> {{ doc.name }} </b> </span></p>\r\n\t\t{% elif doc.is_return and not doc.is_paid %}\r\n            <p style=\" margin-left:30mm; display:\"> \u0641\u0627\u062a\u0648\u0631\u0629 \u0645\u0631\u062f\u0648\u062f\u0627\u062a \u0645\u0628\u064a\u0639\u0627\u062a (\u0622\u062c\u0644\u0629) \u0631\u0642\u0645 <span> <b> {{ doc.name }} </b> </span></p>\r\n\t\t{% elif not doc.is_return and doc.is_paid %}\r\n            <p style=\" margin-left:30mm; display:\"> \u0641\u0627\u062a\u0648\u0631\u0629 \u0645\u0628\u064a\u0639\u0627\u062a (\u0646\u0642\u062f\u064a\u0629) \u0631\u0642\u0645 <span> <b> {{ doc.name }} </b> </span></p>\r\n\t\t{% elif not doc.is_return and not doc.is_paid %}\r\n            <p style=\" margin-left:30mm; display:\"> \u0641\u0627\u062a\u0648\u0631\u0629 \u0645\u0628\u064a\u0639\u0627\u062a (\u0622\u062c\u0644\u0629) \u0631\u0642\u0645 <span> <b> {{ doc.name }} </b> </span></p>\r\n        {% endif %}\r\n        <p style=\" display: inline-block;\"> <span> <b> </b></span> </p>\r\n        \r\n    </div>\r\n</div>\r\n<div class=\"row\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n\t\t<table class=\"table table-condensed table-bordered cart \" style=\"margin-top:1mm;\">\r\n        \t<tbody>\r\n        \t\t<tr>\r\n        \t\t\t<td>\r\n            \t\t\t<div>\r\n                           <label style=\"display: inline-block !important;\">\u0627\u0644\u0639\u0645\u064a\u0644</label>\r\n                        </div>\r\n                    </td>\r\n                    <td colspan=\"3\">\r\n                        <div>\r\n                        \t {{ doc.customer_name }}\r\n                        </div>\r\n                    </td>\r\n        \t\t\t<td>\r\n        \t\t\t   <div>\r\n                        \t<label style=\"display: inline-block !important;\">\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0627\u0635\u062f\u0627\u0631</label>\r\n                        </div>\r\n        \t\t\t</td>\r\n        \t\t\t<td>\r\n        \t\t\t   <div>\r\n                    \t\t{{ doc.posting_date }}\r\n                        </div>\r\n        \t\t\t</td>\r\n        \t\t</tr>\r\n        \t\t\r\n    \t\t\t<tr>\r\n        \t\t\t<td>\r\n        \t\t\t   <div>\r\n                        \t<label style=\"display: inline-block !important; white-space: nowrap;\">\u0623\u0645\u0631 \u0627\u0644\u0628\u064a\u0639</label>\r\n                        </div>\r\n        \t\t\t</td>\r\n        \t\t\t<td colspan=\"3\">\r\n        \t\t\t   <div>\r\n\t\t\t\t\t\t\t{{ doc.items[0].sales_order }}\r\n                        </div>\r\n        \t\t\t</td>\r\n        \t\t\t<td>\r\n\t\t\t\t\t   <div>\r\n\t\t\t\t\t\t\t<label style=\"display: inline-block !important;\">\u0627\u0644\u0645\u0646\u0637\u0642\u0629</label>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</td>\r\n\t\t\t\t\t<td>\r\n\t\t\t\t\t   <div>\r\n\t\t\t\t\t\t\t{{ frappe.db.get_value(\"Customer\",doc.customer, \"territory\") }}\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</td>\r\n        \t\t</tr>\r\n        \t\t<tr>\r\n\t\t\t\t\t<td>\r\n\t\t\t\t\t\t<div>\r\n\t\t\t\t\t\t\t<label style=\"display: inline-block !important; white-space: nowrap;\">\u0625\u0630\u0646 \u0627\u0644\u062a\u0633\u0644\u064a\u0645</label>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</td>\r\n\t\t\t\t\t<td colspan=\"3\">\r\n\t\t\t\t\t\t<div>\r\n\t\t\t\t\t\t\t{{ doc.items[0].delivery_note }}\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</td>\r\n\t\t\t\t\t<td>\r\n            \t\t\t<div>\r\n                           <label style=\"display: inline-block !important;\">\u0627\u0644\u062a\u0644\u064a\u0641\u0648\u0646</label>\r\n                        </div>\r\n                    </td>\r\n                    <td >\r\n                        <div>\r\n    \t\t\t            {{ frappe.db.get_value(\"Customer\",doc.customer, \"phone_no\") }}\r\n                        </div>\r\n                    </td>\r\n        \t\t</tr>\r\n        \t\t<tr>\r\n        \t\t    <td>\r\n        \t\t        <label style=\"display: inline-block !important;\">\u0627\u0644\u0645\u062e\u0632\u0646</label>\r\n        \t\t    </td>\r\n        \t\t    <td colspan=\"7\">\r\n        \t\t        {{ doc.set_warehouse }}\r\n        \t\t    </td>\r\n        \t\t</tr>\r\n        \t</tbody>\r\n        </table>\r\n\t</div>\r\n</div>\r\n<div class=\"row\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n        <table class=\"table table-condensed table-bordered cart text-center \" style=\"margin-top: 0px\">\r\n        \t<thead>\r\n        \t\t<tr>\r\n        \t\t\t<th style=\" text-center\"><b>\u0645</b></th>\r\n        \t\t\t<th style=\" text-center\"><b>\u0627\u0644\u0645\u0627\u0631\u0643\u0629</b></th>\r\n        \t\t\t<th style=\" text-center\"><b>\u0627\u0644\u0635\u0646\u0641</b></th>\r\n        \t\t\t<th style=\" text-center\"><b>\u0627\u0644\u0643\u0645\u064a\u0629</b></th>\r\n                    <th style=\" text-center\"><b>\u0627\u0644\u0633\u0639\u0631</b></th>\r\n        \t\t\t<th style=\" text-center\"><b>\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a</b></th>\r\n        \t\t</tr>\r\n        \t</thead>\r\n        \t<tbody>\r\n        \t    \r\n        \t{%- for item in doc.items -%}\r\n            <tr>\r\n    \t\t\t<td>{{ loop.index }}</td>\r\n    \t\t\t<td>{{ item.brand }}</td>\r\n    \t\t\t<td>{{ item.item_name }}</td>\r\n    \t\t\t<td>{{ frappe.format_value( item.qty | abs, {\"fieldtype\": \"Float\", \"precision\": 0}) }} </td>\r\n                <td>{{ frappe.format_value( item.rate | abs, {\"fieldtype\": \"Currency\", \"precision\": 2}) }}</td>\r\n    \t\t\t<td>{{ frappe.format_value( item.amount | abs, {\"fieldtype\": \"Currency\", \"precision\": 2}) }}</td>\r\n    \t\t</tr>\r\n        \t{%- endfor -%}\r\n    \t</tbody>\r\n    </table>\r\n</div>\r\n</div>\r\n<div class=\"row\">\r\n\t<div class=\"col-xs-12 column-break\">\r\n    \t<table class=\"table table-condensed table-bordered cart \">\r\n        \t<tbody class=\"text-center\">\r\n    \t\t\t<tr>\r\n        \t\t    <td><b>\u0627\u0644\u0643\u0645\u064a\u0629</b></td>\r\n        \t\t    <td><b>\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a</b></td>\r\n        \t\t    <td><b>\u0627\u0644\u062e\u0635\u0645</b></td>\r\n        \t\t    <td><b>\u0627\u0644\u0635\u0627\u0641\u064a \u0628\u0639\u062f \u0627\u0644\u062e\u0635\u0645</b></td>\r\n                {% if doc.is_return %}\r\n        \t\t    <td><b>\u0627\u0644\u0645\u062f\u0641\u0648\u0639 \u0644\u0644\u0639\u0645\u064a\u0644</b></td>\r\n        \t\t    <td><b>\u0645\u0628\u0644\u063a \u0627\u0644\u0625\u0636\u0627\u0641\u0629</b></td>\r\n                {% else %}\r\n        \t\t    <td><b>\u0627\u0644\u0645\u062d\u0635\u0644 \u0645\u0646 \u0627\u0644\u0639\u0645\u064a\u0644</b></td>\r\n        \t\t    <td><b>\u0645\u0628\u0644\u063a \u0627\u0644\u0625\u0639\u0641\u0627\u0621</b></td>\r\n                {% endif %}\r\n        \t\t</tr>\r\n        \t\t<tr>\r\n        \t\t\t<td>{{ frappe.format_value( doc.total_qty | abs, {\"fieldtype\": \"Float\", \"precision\": 0}) }}</td>\r\n        \t\t\t<td>{{ frappe.format_value( doc.total | abs, {\"fieldtype\": \"Currency\", \"precision\": 2}) }}</td>\r\n        \t\t\t<td>{{ frappe.format_value( doc.discount_amount | abs, {\"fieldtype\": \"Currency\", \"precision\": 2}) }}</td>\r\n                    <td>{{ frappe.format_value( doc.grand_total | abs, {\"fieldtype\": \"Currency\", \"precision\": 2}) }}</td>\r\n            {% if doc.is_paid %}\r\n\t\t\t    {% if doc.custom_payment_value_is_different %}\r\n\t\t\t        <td>{{ frappe.format_value(doc.custom_payment_value | abs, {\"fieldtype\": \"Currency\", \"precision\": 2}) }}</td>\r\n\t\t\t        <td>{{ frappe.format_value((doc.grand_total | abs) - doc.custom_payment_value, {\"fieldtype\": \"Currency\", \"precision\": 2}) }}</td>\r\n\t\t\t    {% else %}\r\n\t\t\t        <td>{{ frappe.format_value(doc.grand_total | abs, {\"fieldtype\": \"Currency\", \"precision\": 2})  }}</td>\r\n\t\t\t        <td>0</td>\r\n\t\t\t    {%endif%}\r\n            {% else %}\r\n                    <td>0</td>\r\n                    <td>0</td>\r\n            {% endif %}\r\n        \t\t</tr>\r\n        \t</tbody>\r\n        </table>\r\n\t\t<div class=\"row section-break text-center\">\r\n\t\t    <p>{{ _(doc.in_words) }}</p>\r\n\t\t</div>\r\n    </div>\r\n</div>\r\n<div class=\"row text-center\" style=\"margin-top:10px;\">\r\n    <div class=\"col-xs-4 column-break left-align\">\r\n        <div>\u062a\u0648\u0642\u064a\u0639 \u0648\u062e\u062a\u0645 \u0627\u0644\u0634\u0631\u0643\u0629</div>\r\n    </div>\r\n    <div id=\"footer-html\" class=\"visible-pdf\">\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</div>",
