 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header,\n\t render_field -%}\n\n\n<div id=\"header-html\" >\n    <div class=\"letter-head\">{{ letter_head }}</div>\n    <div style=\"\">\n    {% if doc.type == 'Add' %}\n        <p style=\" margin-left:30mm;\"> \u0633\u0646\u062f \u062f\u064a\u0646 (\u0625\u0636\u0627\u0641\u0629) \u0631\u0642\u0645<span> <b> {{ doc.name }} </b> </span></p>\n    {% else %}\n        <p style=\" margin-left:30mm;\"> \u0633\u0646\u062f \u062f\u064a\u0646 (\u0625\u0639\u0641\u0627\u0621) \u0631\u0642\u0645<span> <b> {{ doc.name }} </b> </span></p>\n    {% endif %}\n    </div>\n</div>\n<div class=\"row\">\n\t<div class=\"col-xs-12 column-break\">\n\t\t<table class=\"table table-condensed table-bordered cart \" style=\"margin-top:1mm;\">\n        \t<tbody>\n        \t\t<tr>\n        \t\t\t<td>\n            \t\t\t<div>\n                        {% if doc.type == 'Add' %}\n                           <label style=\"display: inline-block !important; white-space: nowrap;\">\u0625\u0636\u0627\u0641\u0629 \u0625\u0644\u0649</label>\n                        {% else %}\n                           <label style=\"display: inline-block !important; white-space: nowrap;\">\u0625\u0639\u0641\u0627\u0621 \u0645\u0646</label>\n                        {% endif %}\n                        </div>\n                    </td>\n                    <td colspan=\"3\">\n                        <div>\n                        {% if doc.party_type %}\n                        \t {{ doc.party_name }}\n                        {% else %}\n                            {{ frappe.db.get_value('Account', doc.from_or_to_account, 'account_name') }}\n                        {% endif %}\n                        </div>\n                    </td>\n        \t\t\t<td>\n        \t\t\t   <div>\n                        <label style=\"display: inline-block !important;\">\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0625\u0635\u062f\u0627\u0631</label>\n                        </div>\n        \t\t\t</td>\n        \t\t\t<td>\n        \t\t\t   <div>\n                    \t{{ doc.posting_date }}\n                        </div>\n        \t\t\t</td>\n        \t\t</tr>\n        \t</tbody>\n        </table>\n\t</div>\n</div>\n<div class=\"row\">\n\t<div class=\"col-xs-12 column-break\">\n    \t<table class=\"table table-condensed table-bordered cart \">\n        \t<tbody class=\"text-center\">\n    \t\t\t<tr>\n                {% if doc.type == 'Add' %}\n        \t\t    <td><b>\u0645\u0628\u0644\u063a \u0627\u0644\u0625\u0636\u0627\u0641\u0629</b></td>\n                {% else %}\n         \t\t    <td><b>\u0645\u0628\u0644\u063a \u0627\u0644\u0625\u0639\u0641\u0627\u0621</b></td>\n        \t    {% endif %}\n        \t\t</tr>\n        \t\t<tr>\n                    <td>{{ frappe.format_value( doc.amount, {\"fieldtype\": \"Currency\", \"precision\": 2})  }}</td>\n        \t\t</tr>\n        \t</tbody>\n        </table>\n\t\t<div class=\"row section-break text-center\">\n\t\t</div>\n    </div>\n</div>\n<div class=\"row text-center\" style=\"margin-top:10px\">\n    <div class=\"col-xs-4 column-break\">\n        <div>\u062a\u0648\u0642\u064a\u0639 \u0648\u062e\u062a\u0645 \u0627\u0644\u0634\u0631\u0643\u0629</div>\n    </div>\n    </div>\n\t<div id=\"footer-html\" class=\"visible-pdf\">\n\t\t<div class=\"letter-head-footer\">\n\t\t\t{{ footer }}\n\t\t</div>\n\t\t<p class=\"text-center small page-number visible-pdf\">\n\t\t\t{{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n\t\t</p>\n\t</div>",
